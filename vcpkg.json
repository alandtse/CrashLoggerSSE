{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "crashloggersse",
  "version-string": "1.19.2",
  "port-version": 0,
  "description": "SKSE/SKSEVR plugin that generates crash logs when the game Just Worksâ„¢.",
  "homepage": "https://github.com/alandtse/CrashLoggerSSE",
  "license": "MIT",
  "features": {
    "plugin": {
      "description": "Build the SKSE plugin.",
      "dependencies": [
        "boost-stacktrace",
        "clib-util",
        "directxtk",
        "frozen",
        {
          "name": "infoware",
          "features": [
            "d3d"
          ]
        },
        "magic-enum",
        "openvr",
        "rapidcsv",
        "rsm-binary-io",
        "spdlog",
        "vmaware",
        "zycore",
        "zydis"
      ]
    },
    "tests": {
      "description": "Build tests for the SKSE plugin.",
      "dependencies": [
        {
          "name": "crashloggersse",
          "default-features": false,
          "features": [
            "plugin"
          ]
        },
        "catch2"
      ]
    }
  },
  "default-features": [
    "plugin"
  ],
  "builtin-baseline": "7a6bb52bfd8eba088f8bf5cae70ca8d81503e42e",
  "overrides": [
    {
      "name": "fmt",
      "version": "11.0.2"
    },
    {
      "name": "spdlog",
      "version": "1.15.0"
    }
  ]
}
